import React, { Component } from 'react';
import { styles } from './styles.js';

/*const grey = '#606060';
const style = {
	color: grey,
	fontFamily: styles.fontFamily
};*/
const txt = "PS. Love CapCher <3"

const love = 'pink url("https://i.pinimg.com/originals/18/61/29/18612970368b6f38ded93ccdb96d18d5.jpg")fixed';
const cool = 'pink url("data:image/jpeg;base64,")fixed';
const gif1 = "https://media1.tenor.com/images/234c0bd4608b981c3299adb6e2b90bc1/tenor.gif?itemid=10679098";
const gif2 = "https://thumbs.gfycat.com/RedImpassionedGlobefish-max-1mb.gif";
const styleColor = {
	color: styles.color,
	fontFamily: styles.fontFamily,
	fontSize: styles.fontSize
};
class App extends Component {
	constructor(props){
	super(props)
	this.state = { feel: love,
					src: gif1};
	this.update = this.update.bind(this);	
	}
	update(){
		const newFeel = this.state.feel === love ? cool : love;
		const newSrc = this.state.feel === love ? gif2 : gif1;
		this.setState({feel: newFeel , src: newSrc});

	}
	render() {
		
		return(
			<div style={{background: this.state.feel , textAlign:"center"}}>
				<h1 style={styleColor}>
					CapCher Background Changer
				</h1>
				<img src={this.state.src} />
				<br>
				</br>
				<button onClick={this.update}>
				Change background!
				</button>
				<h1 style={styleColor}>
				 First time with React and this is what I made LOL <br></br>
				{txt} 
				</h1>
			</div>
		)	
	  }
}

export default App